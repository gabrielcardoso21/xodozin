{
  "name": "xodozin",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "node --max-old-space-size=2048 node_modules/.bin/medusa build",
    "build:backend-only": "tsc --build",
    "build:skip-if-exists": "bash scripts/build-skip-if-exists.sh",
    "build:compile": "tsc --build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa db:migrate && medusa start",
    "start:skip-build": "medusa db:migrate && medusa start",
    "start:production": "medusa db:migrate && medusa start",
    "dev": "medusa develop",
    "setup:brasil": "medusa exec ./src/scripts/setup-brasil.ts",
    "setup:payment": "medusa exec ./src/scripts/setup-payment-provider.ts",
    "setup:users": "medusa exec ./src/scripts/create-users-final.ts",
    "reset:passwords": "medusa exec ./src/scripts/reset-password.ts",
    "delete:users": "medusa exec ./src/scripts/delete-users.ts",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.11.3",
    "@medusajs/cli": "2.11.3",
    "@medusajs/framework": "2.11.3",
    "@medusajs/medusa": "2.11.3",
    "@types/bcryptjs": "^3.0.0",
    "bcryptjs": "^3.0.3",
    "resend": "^4.0.0"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.11.3",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
